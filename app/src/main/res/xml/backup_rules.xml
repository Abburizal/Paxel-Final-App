<full-backup-content>
    <!-- Include measurement database -->
    <include domain="database" path="." />

    <!-- Include shared preferences -->
    <include domain="sharedpref" path="." />

    <!-- Include photos directory -->
    <include domain="external" path="Pictures" />

    <!-- Exclude cache directories -->
    <exclude domain="cache" />
    <exclude domain="external-cache" />
</full-backup-content>