<data-extraction-rules>
    <cloud-backup>
        <!-- Backup measurement data -->
        <include domain="database" path="." />
        <include domain="sharedpref" path="." />
        <!-- Exclude temporary files -->
        <exclude domain="cache" />
        <exclude domain="external-cache" />
    </cloud-backup>

    <device-transfer>
        <!-- Transfer user data but not cache -->
        <include domain="database" path="." />
        <include domain="sharedpref" path="." />
        <exclude domain="cache" />
    </device-transfer>
</data-extraction-rules>